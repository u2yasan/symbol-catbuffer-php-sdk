{
  "name": "symbol-blockchain-community/symbol-catbuffer-php-sdk",
  "description": "Symbol catbuffer-based PHP SDK (PHP 8.3, PSR-12, strict_types, immutable).",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": { "SymbolSdk\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": {
      "SymbolSdk\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^10",
    "phpstan/phpstan": "^1.11"
  },
  "scripts": {
    "analyse": "phpstan analyse",
    "test": "phpunit --colors=always"
  }
}
